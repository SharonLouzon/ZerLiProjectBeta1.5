               </GridPane.margin></Button>
            <CheckBox fx:id="compensation" mnemonicParsing="false" prefHeight="17.0" prefWidth="233.0" text="Compensate?" GridPane.rowIndex="3" />
            <TextField fx:id="amount" promptText="Compensation Amount" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </GridPane>
   </children>
